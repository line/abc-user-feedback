{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "next dev",
    "format": "prettier --check \"./src/**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "format:fix": "prettier --write --list-different \"./src/**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "generate-api-type": "openapi-typescript http://0.0.0.0:4000/admin-docs-json --output src/types/api.type.ts",
    "lint": "set SKIP_ENV_VALIDATION=1 && next lint",
    "start": "next start",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --ci --passWithNoTests",
    "test:dev": "jest --watch --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@ufb/prettier-config",
  "eslintConfig": {
    "extends": [
      "@ufb/eslint-config/base",
      "@ufb/eslint-config/react",
      "@ufb/eslint-config/nextjs"
    ],
    "ignorePatterns": [
      "api.type.ts"
    ],
    "root": true
  },
  "dependencies": {
    "@faker-js/faker": "^8.2.0",
    "@floating-ui/react": "^0.26.1",
    "@headlessui/react": "1.7.17",
    "@headlessui/tailwindcss": "^0.2.0",
    "@hookform/resolvers": "^3.3.4",
    "@mui/base": "^5.0.0-beta.23",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-table": "^8.11.8",
    "@ufb/shared": "workspace:*",
    "@ufb/tailwind": "workspace:*",
    "@ufb/ui": "workspace:*",
    "axios": "^1.6.7",
    "axios-auth-refresh": "^3.3.6",
    "classnames": "^2.5.1",
    "clsx": "^2.1.0",
    "cookies-next": "^4.0.0",
    "countries-and-timezones": "^3.6.0",
    "date-fns": "^3.0.6",
    "dayjs": "^1.11.10",
    "framer-motion": "^10.16.4",
    "i18next": "^23.9.0",
    "immer": "^10.0.3",
    "iron-session": "^6.3.1",
    "jwt-decode": "^4.0.0",
    "next": "^14.0.3",
    "next-i18next": "^14.0.3",
    "pino": "^8.16.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-datepicker": "^4.21.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.3.1",
    "react-select": "^5.7.7",
    "react-use": "^17.4.0",
    "recharts": "^2.12.0",
    "sharp": "^0.32.6",
    "tailwind-scrollbar-hide": "^1.1.7",
    "zod": "^3.22.4",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@svgr/webpack": "^8.1.0",
    "@swc/core": "^1.3.99",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.12",
    "@types/node": "20.8.8",
    "@types/react": "^18.2.55",
    "@types/react-beautiful-dnd": "^13.1.6",
    "@types/react-datepicker": "^4.19.1",
    "@types/react-dom": "^18.2.18",
    "@ufb/eslint-config": "workspace:*",
    "@ufb/prettier-config": "workspace:*",
    "@ufb/tsconfig": "workspace:*",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "openapi-typescript": "^6.7.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^5.4.3"
  }
}
