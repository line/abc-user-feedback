    "start": "next start",
    "test": "SKIP_ENV_VALIDATION=1 jest --passWithNoTests",
    "test:snapshot": "jest -u",
    "test:ci": "jest --ci --passWithNoTests",
    "test:watch": "SKIP_ENV_VALIDATION=1 jest --watch --passWithNoTests",
    "typecheck": "tsc --noEmit"
