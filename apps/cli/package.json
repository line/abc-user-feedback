{
  "name": "cli",
  "version": "1.0.0",
  "bin": "dist/cli.js",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf dist .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "format:fix": "prettier --write --list-different \"./src/**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "lint": "eslint",
    "start": "node dist/ufb-cli.js",
    "start:dev": "ts-node bin/ufb-cli.ts"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "commander": "^12.1.0",
    "inquirer": "^10.1.8",
    "js-toml": "^1.0.0"
  },
  "devDependencies": {
    "@ufb/eslint-config": "workspace:*",
    "@ufb/prettier-config": "workspace:*",
    "@ufb/tsconfig": "workspace:*",
    "ts-node": "^10.9.2"
  },
  "prettier": "@ufb/prettier-config"
}
