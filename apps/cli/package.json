{
  "name": "ufb-cli",
  "version": "1.0.16",
  "bin": {
    "ufb-cli": "./dist/ufb-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf dist .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "format:fix": "prettier --write --list-different .",
    "lint": "eslint",
    "start": "node dist/ufb-cli.js",
    "start:dev": "ts-node bin/ufb-cli.ts"
  },
  "dependencies": {
    "@types/prompts": "^2.4.9",
    "child_process": "^1.0.2",
    "commander": "^12.1.0",
    "js-toml": "^1.0.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@ufb/eslint-config": "workspace:*",
    "@ufb/prettier-config": "workspace:*",
    "@ufb/tsconfig": "workspace:*",
    "ts-node": "^10.9.2"
  },
  "prettier": "@ufb/prettier-config"
}
