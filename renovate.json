{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "ignorePaths": [".github/workflows/e2e-test.yml"],
  "baseBranches": ["dev"],
  "packageRules": [
    {
      "matchPackagePatterns": ["^@ufb/"],
      "enabled": false
    }
  ],
  "automerge": true,
  "docker-compose": {
    "digest": {
      "enabled": false
    }
  }
}
