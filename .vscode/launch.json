{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "api",
      "type": "node-terminal",
      "request": "launch",
      "command": "pnpm dev",
      "cwd": "${workspaceFolder}/apps/api",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "sourceMaps": true,
    },
    {
      "name": "web",
      "type": "node-terminal",
      "request": "launch",
      "command": "pnpm dev",
      "cwd": "${workspaceFolder}/apps/web",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "/turbopack/[project]/*": "${webRoot}/*"
      }
    }
  ]
}