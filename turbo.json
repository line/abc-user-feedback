{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"],
      "env": [
        "NODE_ENV",
        "SESSION_PASSWORD",
        "API_BASE_URL",
        "NEXT_PUBLIC_API_BASE_URL",
        "OS_USE",
        "OS_NODE",
        "OS_USERNAME",
        "OS_PASSWORD",
        "APP_PORT",
        "APP_ADDRESS",
        "JWT_SECRET",
        "MYSQL_PRIMARY_URL",
        "MYSQL_SECONDARY_URLS",
        "SMTP_USE",
        "SMTP_HOST",
        "SMTP_PORT",
        "SMTP_USERNAME",
        "SMTP_PASSWORD",
        "SMTP_SENDER",
        "SMTP_BASE_URL",
        "AUTO_MIGRATION",
        "MASTER_API_KEY",
        "BASE_URL",
        "NEXT_PUBLIC_MAX_DAYS"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["@ufb/ui#build"],
      "cache": false,
      "persistent": true
    },
    "test": {},
    "@ufb/tailwind#build": {
      "outputs": ["dist/**"]
    },
    "@ufb/ui#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["@ufb/tailwind#build"]
    }
  }
}
